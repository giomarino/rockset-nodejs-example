#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules/cdk-assets/bin/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules/cdk-assets/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules/cdk-assets/bin/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules/cdk-assets/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/cdk-assets@2.62.2/node_modules:/Users/gmarino/Workspace/rockset-client-nodejs-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cdk-assets" "$@"
else
  exec node  "$basedir/../../bin/cdk-assets" "$@"
fi
